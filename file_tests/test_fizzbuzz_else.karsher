count = 0

while (count <= 100) {
    text = ""
    if(count % 3 == 0 && count % 5 == 0) {
      # Fizz Buzz
       toprint = true
       text = count + " = FizzBuzz"
       println(text)

    } else if(count % 5 == 0) {
      # buzz
       toprint = true
       text = multiline {
          count + 
          " = Buzz" # oops
        }
       println(text)
    } else if(count % 3 == 0) {
      #fizz
        text = multiline {
                  count
                  + 
                  " = Fizz"
        }
       println(text)

    }
