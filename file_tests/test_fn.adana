map = () => {
    []
}

push_v = (key, value, map) => {
    [[key, value]] + map
}


get_v = (key, map) => {
    count = 0
    res = [] # simulate null
    while(count < length(map)) {
        k = map[count]
        if(k[0] == key) {
            res = k[1] 
            count = length(map) # simulate break / early return
        } else {
            count = count +1
        }
    }
    res
}